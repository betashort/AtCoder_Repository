   "source": [
    "#最大公約数\n",
    "def gcd(a,b):\n",
    "    while b!=0:\n",
    "        a,b=b,a%b\n",
    "    return a\n",
    "\n",
    "#最小公倍数\n",
    "def lcm(a,b):\n",
    "    return a*b//gcd(a,b)\n"
   ]
  },
